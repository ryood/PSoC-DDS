                  <description>Internal only</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>10x</name>
                  <description>Output to pin</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
